{
  "schemaVersion": 1,
  "id": "coffee",
  "version": "${version}",
  "name": "Coffee",
  "description": "Ever wanted to bring your caffeine addiction to Minecraft? Now you can!",
  "authors": ["Aznos"],
  "contact": {
    "sources": "https://github.com/Aznos/CoffeeMod",
    "issues": "https://github.com/Aznos/CoffeeMod/issues"
  },
  "license": "ARR",
  "icon": "assets/coffee/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.aznos.coffee.client.CoffeeClient"
    ],
    "main": [
      "com.aznos.coffee.Coffee"
    ],
    "fabric-datagen": [
      "com.aznos.coffee.CoffeeDataGenerator"
    ]
  },
  "mixins": [
    "coffee.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
